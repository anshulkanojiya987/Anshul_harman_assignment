package A;
import java.util.*;
public class GradeAverage {
	public static void main (String args[])
	{
		Scanner scn = new Scanner(System.in);
		System.out.println("Enter the number of students");
		int n = scn.nextInt();
		int a[];
		a = new int[n];
		for(int i=0; i<n; i++)  
		{  
		//reading array elements from the user   
		a[i]=scn.nextInt();  
		}
		for(int i=0; i<n; i++)  
		{  
		//reading array elements from the user   
		System.out.print(a[i]);  
		}
	}

}
